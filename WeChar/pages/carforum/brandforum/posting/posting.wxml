<!--pages/carforum/brandforum/brandforuminfo/forumpost/posting/posting.wxml-->
<form bindsubmit='posting' data-brandid='{{brandid}}'>
<view class='forumHead'>
<view><text class='title'>{{title}}</text></view>
</view>
<text>\n</text>
<view class='content'>
<text class='fontStyle'>标题</text><input class='postTitle' name='postTitle' value='{{postTitle}}'></input>
<text>\n</text><text>\n</text>
<text class='fontStyle'>内容</text><input class='postContent' name='postContent' value='{{postContent}}'></input>
<button  form-type='submit'>发帖</button>
</view>
</form>